<div class="mb-10 border-t border-b divide-y">
    <% for (const plane of planes) { %>
      <div class="grid py-8 sm:grid-cols-4 gap-4">
        <div class="mb-4 sm:mb-0">
          <div class="space-y-1 text-xs font-semibold tracking-wide uppercase">
            <!-- Tipo -->
            <p class="text-gray-400" aria-label="Category">
              <%= plane.typePlane %>
            </p>
            <!-- Modelo -->
            <p class="text-gray-400" aria-label="Category">
              <%= plane.modelPlane %>
            </p>
            <!-- Año -->
            <p class="text-gray-200"><%= plane.yearPlane %></p>
            <!-- País -->
            <p class="text-gray-200"><%= plane.countryPlane %></p>
          </div>
        </div>
        <div class="sm:col-span-3 lg:col-span-2">
          <div class="mb-3">
            <!-- Nombre -->
            <p class="text-3xl font-extrabold leading-none sm:text-4xl xl:text-4xl">
              <%= plane.namePlane %>
            </p>
          </div>
          <!-- Descripción -->
          <p class="text-gray-200">
            <%= plane.descriptionPlane %>
          </p>
        </div>
      </div>
    <% } %>
  </div>
  